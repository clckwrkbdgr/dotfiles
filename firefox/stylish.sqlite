    opacity: 1;
  }

  #forum td.created, #forum td.last-reply
  {
    font-size: 80%;
  }

  #forum .name
  {
    font-weight: 700;
    font-size: 1.1em;
  }

  .topic-icon, .topic-topic, .topic-replies, .topic-views, .topic-created, .topic-lreply
  {
    color: #999;
  }

  .picture
  {
    padding: 0;
    width: 70px;
    height: 70px;
    border-radius: 40px;
    overflow: hidden;
    border: 3px solid #555;
    background: #222;
    filter: grayscale(50%);
  } /*ava*/
  .picture:after
  {
    content: ''☠'';
    font-size: 50px;
    display: block;
    width: 70px;
    height: 70px;
    padding: 23px 11px;
    margin: 0;
    color: #666;
  }

  .profile h3
