  h1, h2 { font-size: 10pt !important; }
  #chatPage div#header { height: 30px !important; }
  #sysChat .inside { padding-top: 0px !important; }
  div.disconnected div { margin: 0 0 0 0 !important; height: auto !important; }
  iframe#vkwidget4 { height: 0 !important; }
  .chatmsg { border: 0 !important; }
  body#chatWindow { border-image-width: 0 0 0 0 !important; border-bottom-color: black !important; }
  div#wrapper, div#chatPage, div.chatContent { padding: 0 0 0 0 !important; margin: 0 0 0 0 !important; }
  div.controlwrapper
  {
