}

@font-face
{
  font-family: ''PT Sans'';  font-style: italic;  font-weight: 400;  src: local(''PT Sans Italic''), local(''PTSans-Italic''), url(https://fonts.gstatic.com/s/ptsans/v8/7dSh6BcuqDLzS2qAASIeultXRa8TVwTICgirnJhmVJw.woff2) format(''woff2'');
}

@font-face
{
  font-family: ''PT Sans'';  font-style: normal;  font-weight: 700;  src: local(''PT Sans Bold''), local(''PTSans-Bold''), url(https://fonts.gstatic.com/s/ptsans/v8/0XxGQsSc1g4rdRdjJKZrNPk_vArhqVIZ0nv9q090hN8.woff2) format(''woff2'');
}

@-moz-document
  domain("flibusta.is"),
  domain("flibusta.me"),
  domain("flibusta.net"),

  regexp("https?://(proxy\\.|www\\.)?flibusta\\.net/b/\\d+/read.*"),
  regexp("http://flibustahezeous3.onion/"),
  regexp("https?://flibusta\\.i2p/b/\\d+/read.*"),
  regexp("https?://zmw2cyw2vj7f6obx3msmdvdepdhnw2ctc4okza2zjxlukkdfckhq\\.b32\\.i2p/b/\\d+/read.*")
{
  
  html, body, table th, table td
  {
    font-family: ''PT sans'',sans-serif;
    line-height: 1.6em;
    font-size: 15px;
    letter-spacing: 0;
    color: #777;
  }

  body, #page, #front, #second
  {
    background: #090909;
  }

  #divabio
  {
    background: none !important;
    border: none !important;
    box-shadow: 0 0 25px #111;
    padding: 15px !important;
    border-radius: 5px;
    margin-bottom: 20px !important;
  }

  #container
  {
    transition: .3s all ease-in-out;
    background: #111;
    padding: 30px 20px;
    box-shadow: 0 0 25px #000;
    border-radius: 5px;
  }

  #page
  {
    width: 80%;
    margin: 0 auto;
  }

  #header #logo
  {
    opacity: .6;
  }

  #header
  {
    background: none;
    margin: 0px 0 0 0;
    height: 90px;
  }

  #header #site-name, #header #site-slogan
  {
    top: 35px;
  }

  #header #site-name a, #header #site-name a:visited
  {
    letter-spacing: 0;
    font-size: 1.5em;
    color: #666;
    font-family: ''PT sans'', sans-serif;
    font-weight: 700;
    text-shadow: 0 0 10px #000;
  }

  #header #site-name a:hover
  {
    letter-spacing: 0;
    font-size: 1.5em;
    color: #999;
    font-family: ''PT sans'', sans-serif;
    font-weight: 700;
    text-shadow: 0 0 3px #000;
  }

  #header #site-slogan
  {
    letter-spacing: 0;
    font-size: .7em;
    top: 30px;
    padding: 0;
    margin: 0 0 0 12px;
    color: #444;
    font-weight: 700;
    text-shadow: 0 0 3px #000;
  }

  #mission
  {
    padding: 4px 8px;
    border: 0;
    background: none;
    font-size: 1em;
    border-radius: 5px;
    font-style: italic;
  }

  #main
  {
    overflow: hidden;
    background: none;
  }

  #main-wrapper
  {
    margin: 0px 0;
  }

  /*табличка ABC*/ 
  #block-librusec-abc table td
  {
    padding: 0;
    margin: 0;
  }

