  a.btn span, a.btn span span, div#sysChat .inside, #header p strong#online_counter { background-color: #111 !important; background-image: none !important; }
  body, div, p, table, tr, td, article, section, figure, ul, textarea, sup { color:#bbb !important; background-color: #111 !important; }
  li, hr { color: #fff !important; }
