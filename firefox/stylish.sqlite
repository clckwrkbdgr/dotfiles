INSERT INTO "styles" VALUES(35,NULL,'',NULL,'vimperator','@namespace url(http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul);

/*change cmd box*/
#liberator-commandline-command{color:red!important;background:#111!important}
/*change statusbar*/
#liberator-statusline{color:red!important;background:#111!important;border:1pt solid lightgray!important}

/*change bg of the rest of the statusbar*/
#status-bar{background-color:#111!important}
/*larger cmd line font*/
#liberator-commandline{font-size:10pt!important}
/*add some space to : */
#liberator-commandline-prompt{padding:0 2pt!important}
/*add a border to cmd box*/
#liberator-container{border:1pt solid lightgray!important}
/*change border-color on mouseover*/
#liberator-container:hover,
#liberator-statusline:hover{border-color:#111!important}
/*stuff in the rightside of statusbar*/
#liberator-statusline-field-inputbuffer{color:yellow!important}
#liberator-statusline-field-progress{color:orange!important}
#liberator-statusline-field-tabcount{color:blue!important}
#liberator-statusline-field-bufferposition{color:purple!important}',1,NULL,NULL,1,NULL);
