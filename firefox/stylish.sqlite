  #block-librusec-abc table tr td
  {
    vertical-align: top;
  }

  #block-librusec-abc table tr td a
  {
    white-space: pre;
  }

  #block-librusec-abc table > tbody:nth-child(1) > tr:nth-child(1) > td:nth-child(1) a
  {
    float: left;
  }

  #block-librusec-abc table > tbody:nth-child(1) > tr:nth-child(1) > td:nth-child(2) a
  {
    float: right;
    padding: 0 10px 0 10px;
  } 
 
    /*менюха*/ 
  #primary ul
  {
    position: absolute;
    top: 50px;
    right: 10%;
    margin: 0 auto;
