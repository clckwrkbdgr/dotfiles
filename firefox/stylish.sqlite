    background: #2d3747;
    text-decoration: none;
    text-shadow: 0 0 3px #000;
    color: #999;
  }

  .node div.links
  {
    border-bottom: 3px  dashed #333;
  }

  pre
  {
    background: #222;
    border: 1px solid #444;
  }

  a:link,li a.active,.node h2.title a,   a span.h8
  {
    color: #1e559a;
    text-decoration: underline;
  }

  a:visited
  {
    color: #1e559a;
  }

  a:hover,  a span:hover, .node h2.title a:hover
  {
    color: #57a0fc;
    border: 0;
    opacity: 1 !important;
    text-decoration: none !important;
  }

  a:active,a.active
  {
    color: #57a0fc;
  }

  span.h8
  {
    background: #111;
    padding: 0 6px;
    border-radius: 5px;
  }

  .author-pane a  span
  {
    background: none;
  }

  img
  {
    transition: .5s;
    border-radius: 10px;
  }

  img:hover
  {
    transition: .5s;
  }

  .sticky
  {
    background: none;
    border: 0;
  }

  h1, h2, h3, h4, h5, h6
  {
    padding: 5px 0;
    text-shadow: 0 0 2px #000;
  }

  h1
  {
    font-size: 1.8em;
    border: 0;
  }

  h1.title
  {
    color: #555;
    font-weight: 700;
  }

  h2
  {
    font-size: 1.5em;
  }

  h2.title
  {
    margin: 15px 0;
  }

  .node h2.title a
  {
    font-weight: 700;
  }

  h3
  {
    font-size: 1.2em;
    font-weight: 100;
  }

  h3.title
  {
    font-weight: 700;
  }

  #main > h3
  {
    font-weight: 700;
  }

  h4
  {
    font-size: 1.1em;
    font-weight: 700;
    font-family: monospace;
    letter-spacing: -.6px;
  }

  h5
  {
    font-size: 1em;
    font-weight: 700;
  }

  .sidebar .block h2
  {
    font-weight: 700;
    background: none;
    border: 0;
    font-size: 1.1em;
    color: #888;
  }

  hr
  {
    background: #333 !important;
    overflow: hidden;
    border-top: 2px solid #333 !important;
    border-bottom: 2px solid #333 !important;
    border-radius: 5px !important;
    margin: 8px 0   !important;
  }

  br
  {
    height: 1px;
    display: block;
  }

  .breadcrumb a
  {
    font-size: .8em;
  }
  
  /*lists*/
  ul
  {
    margin: 10px 0;
  }

  ul li, ul.menu li, .item-list ul li,.fb2info-content li, li
  {
    margin: 0;
    list-style: none;
  }

  ul.primary,ul.secondary
  {
    margin: 0;
    padding: 0;
    font-size: .9em;
    border: 0;
  }

  ul.primary li a,ul.secondary li a
  {
    background: none;
    border: 0 !important;
    padding: 4px 6px;
    text-decoration: underline;
  }

  ul.primary li a.active,ul.secondary li.active a, ul.secondary li a:hover
  {
    background: #111;
    box-shadow: inset 0 0 5px #000;
    border: 0;
    color: #1e559a;
    border-radius: 5px;
    text-decoration: none;
    font-weight: 700;
  }

  ul.primary li a:hover
  {
    background: #111;
    box-shadow: inset 0 0 5px #000;
    text-decoration: none;
    color: #1e559a;
    border-radius: 5px;
  }

  ul.primary li.active a
  {
    border: 0 !important;
    background: #111;
    border-radius: 4px;
  }

  ul.secondary li
  {
    padding: 1px 6px;
    border: 0;
  } 

    /*tables*/
  tbody
  {
    border-top: 3px solid #333;
  }

  table
  {
    border-spacing: 0;
    width: 100%;
    line-height: 1.3em;
    border-collapse: separate;
    margin: 30px 0;
    background: none;
    padding: 2px 0;
    empty-cells: hide;
    text-align: left;
  }

  thead th, thead tr
  {
    padding: 4px;
    font-weight: 700;
    border-bottom: 4px solid #444;
  }

  tbody th
  {
    border-bottom: 4px solid #444;
    padding: 0 0 8px 0;
  }

  table td
  {
    padding: 4px 8px;
    font-size: 98%;
  }

  tr.odd
  {
    background: none;
  }

  td.active, tr.selected td
  {
    background: #151515;
  }

  table tr:nth-child(even),   legend.collapse-processed, input, textarea, select, textfield,   .fb2info-content,.book_advise-content,.contentTable-content
  {
    background: #222;
  }

  table.sticky-header
  {
    background: #111;
  }
  
  
  
  /*nn_text*/
  table.sticky-table tr > td:nth-child(1), table.sticky-table tr > td:nth-child(5)
  {
    font-size: .8em;
  }

  li.comment_add a, li.comment_comments a, li.node_read_more a
  {
    background: none;
    padding-left: 2px;
  }
  /*Поиск_расширенный*/
 
  
  

  #tracker td.replies
  {
    text-align: left;
    font-weight: 700;
  }

  td.replies > br
  {
    display: none;
  }

  td.replies a
  {
    font-size: .9em;
    margin: 0 0 0 10px;
    float: right;
  }

  th.active img
  {
    margin: 0 0 0 3px;
  }

  table.forum-topics th
  {
    font-size: .95em;
  }

  table#tt tr:hover, table#tt tr:hover:nth-child(even),thead th,table#tt tr:nth-child(even),#books table tr:hover,#block-librusec-abc table tr:hover, #main > form:nth-child(5) > table:nth-child(1) tr, #newann tr,#newann table td
  {
    background: none !important;
  }

  #main > form:nth-child(5) > table,#main > form:nth-child(5) > table:nth-child(1) > tbody:nth-child(1) > tr:nth-child(2) > td:nth-child(2) > table
  {
    margin: 0;
    padding: 0;
  } /*Рекомендации*/
  table#tt th, table#tt td
  {
    white-space: normal !important;
    line-height: 1.1em;
  }
 
  
  /*ехайнахуй*/
  .forum-jump-links
  {
    display: none;
  } 
  
  /*разговорчики*/  
  blockquote
  {
    border-radius: 5px;
    border: 0;
    padding: 10px;
    background: #111;
  }

  .quote-msg
  {
    width: 80%;
    background: #232323;
    color: #777;
    padding: 10px;
    border-radius: 5px;
    position: relative;
    border: 0;
  }

  .quote-msg:before
  {
    content: "";
    display: block;
    width: 0;
    height: 0;
    border-top: 12px solid transparent;
    border-bottom: 12px solid transparent;
    border-right: 12px solid #232323;
    position: absolute;
    top: 16px;
    left: -12px;
  }

  .quote-msg   .quote-msg
  {
    background: #1f1f1f;
  }

  .quote-msg   .quote-msg:before
  {
    border-right: 12px solid  #1f1f1f;
  }

  .quote-msg   .quote-msg  .quote-msg
  {
    background: #1a1a1a;
  }

  .quote-msg  .quote-msg  .quote-msg:before
  {
    border-right: 12px solid  #1a1a1a;
  }

  .quote-msg   .quote-msg  .quote-msg .quote-msg
  {
    background: #121212;
  }

  .quote-msg  .quote-msg  .quote-msg .quote-msg:before
  {
    border-right: 12px solid  #121212;
  }

  .quote-msg   .quote-msg  .quote-msg .quote-msg .quote-msg
  {
    background: #0d0d0d;
  }

  .quote-msg  .quote-msg  .quote-msg .quote-msg .quote-msg:before
  {
    border-right: 12px solid  #0d0d0d;
  }

  .quote-msg   .quote-msg  .quote-msg .quote-msg .quote-msg  .quote-msg
  {
    background: #080808;
  }

  .quote-msg  .quote-msg  .quote-msg .quote-msg .quote-msg .quote-msg:before
  {
    border-right: 12px solid  #080808;
  }

  .quote-msg   .quote-msg  .quote-msg .quote-msg .quote-msg .quote-msg .quote-msg
  {
    background: #050505;
  }

  .quote-msg  .quote-msg  .quote-msg .quote-msg .quote-msg .quote-msg .quote-msg:before
  {
    border-right: 12px solid  #050505;
  }

  .quote-msg   .quote-msg  .quote-msg .quote-msg .quote-msg .quote-msg .quote-msg  .quote-msg
  {
    background: #000;
    border: 1px solid #000;
  }

  .quote-msg  .quote-msg  .quote-msg .quote-msg .quote-msg .quote-msg  .quote-msg .quote-msg:before
  {
    border-right: 12px solid  #000;
  }

  .quote-msg a
  {
  }

  .quote-msg fieldset
  {
    color: #999;
  }

  .new,.marker
  {
    background: #622424;
    color: #e7c5c5;
    font-size: 80%;
    padding: 2px 4px;
    font-weight: 700;
    line-height: 1.3em;
    border-radius: 5px;
  }

  .node .links,div.meta, .description, .breadcrumb, .submitted, .taxonomy
  {
    font-size: 90%;
  }

  .post-info,.forum-post-panel-sub .author-pane, .reply-count,.forum-folder-legend,.forum-topic-legend
  {
    font-size: 80%;
  }

  .reply-count,.author-pane,.post-title,.description
  {
    font-weight: 700;
  }
 
  
    /*You are not watching this post, click to start watching*/
  .watcher_node
  {
    font-size: 80%;
  }

  .watcher_node_toggle_watching_link
  {
    background: #222;
    padding: 2px 4px;
    border-radius: 5px;
    display: inline;
    margin: 0 20px 0 0;
  }

  .watcher_node_toggle_watching_link_watched
  {
    color: #999 !important;
    background: #31654b !important;
  }

  .author-offline,.author-joined
  {
    color: #555;
  }

  .author-online
  {
    color: #566489;
  }

  ul.tabs.primary
  {
    font-weight: 700;
  }

  #main > div:nth-child(4)
  {
    border: 0 !important;
    font-size: .95em;
  } /*Последние 5 обсуждений*/
  .forum-icon,.forum-folder-legend,.forum-topic-legend
  {
    opacity: .3;
  }

  .forum-icon:hover,.forum-topic-legend:hover
